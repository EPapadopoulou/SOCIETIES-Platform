<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="org.societies.context.user.db.impl.model.UserCtxEntityMgr" table="entities" catalog="userctxdb">
		<id name="operatorId" type="string">
			<column name="operator_id" />
			<generator class="assigned" />
		</id>
		<property name="lastModified" type="date">
			<column name="lastModified" not-null="true" unique="true" />
		</property>		
		<property name="type" type="string">
			<column name="type" length="255" not-null="true" unique="true" />
		</property>
		<property name="objectNumber" type="long">
			<column name="object_number" length="20" not-null="true" unique="true" />
		</property>
    </class>
</hibernate-mapping>