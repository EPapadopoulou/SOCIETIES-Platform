<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.societies.api.schema.useragent.feedback.UserFeedbackBean" table="UserFeedbackBean">
        <!--<meta attribute="class-description">
            This class contains the employee detail.
        </meta>-->
        <id name="requestId" type="string" column="requestId">
            <generator class="uuid"/>
        </id>

        <property name="method" column="method" type="org.societies.api.schema.useragent.feedback.FeedbackMethodType"/>
        <property name="stage" column="stage" type="org.societies.api.schema.useragent.feedback.FeedbackStage"/>
        <property name="requestDate" column="requestDate" type="java.util.Date"/>
        <property name="type" column="type" type="int"/>
        <array name="options" table="UserFeedbackBean_options">
            <key column="option_id"/>
            <index column="option_index"/>
            <element column="value" type="string"/>
        </array>
        <property name="proposalText" column="proposalText" type="string"/>
        <property name="timeout" column="timeout" type="int"/>

    </class>
</hibernate-mapping>
