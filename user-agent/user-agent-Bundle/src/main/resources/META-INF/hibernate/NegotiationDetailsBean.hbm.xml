<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.societies.api.internal.schema.useragent.feedback.NegotiationDetailsBean"
           table="NegotiationDetailsBean">

        <id name="negotiationID" type="string" column="negotiationID">
            <generator class="uuid"/>
        </id>

        <one-to-one name="requestor"
                    class="org.societies.api.schema.identity.RequestorBean"
                    cascade="all"/>

    </class>
</hibernate-mapping>
