<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.societies.api.schema.privacytrust.privacy.model.privacypolicy.ResponseItem"
           table="PrivacyPolicyResponseItem">
        <!--<meta attribute="class-description">
            This class contains the employee detail.
        </meta>-->
        <id name="responseItemId" type="string" column="responseItemId">
            <generator class="uuid"/>
        </id>

        <property name="decision" column="decision"
                  type="org.societies.api.schema.privacytrust.privacy.model.privacypolicy.Decision"/>

        <property name="requestItem" column="requestItem"
                  type="org.societies.api.schema.privacytrust.privacy.model.privacypolicy.RequestItem"/>

    </class>
</hibernate-mapping>
